{
  "name": "nuxt-ecommerce",
  "version": "1.0.0",
  "description": "A whitelabel ecommerce frontend",
  "author": "Jonathan Cowling",
  "private": true,
  "scripts": {
    "dev": "PORT=3000 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "storybook": "start-storybook --port 3000 --static-dir ./static",
    "test": "jest --config test/jest.config.js",
    "integration-test": "jest --config integration-test/jest.config.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/svg": "^0.1.6",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "nuxt-env": "^0.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-backgrounds": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/vue": "^5.2.8",
    "@vue/test-utils": "^1.0.0-beta.27",
    "apollo-server": "^2.9.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "graphql": "^14.5.8",
    "husky": "^2.6.0",
    "jest": "^24.1.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^15.1.1",
    "lint-staged": "^8.2.1",
    "lorem-ipsum": "^2.0.3",
    "prettier": "^1.16.4",
    "svg-inline-loader": "^0.8.0",
    "vue-jest": "^4.0.0-0"
  }
}
